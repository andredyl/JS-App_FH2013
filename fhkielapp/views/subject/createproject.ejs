<%- partial('../partials/header_authenticated')%>

<div id="content-section">
    <div class="page-header">
    <h1>Create Project</h1>
</div>

    <div class="row">
        <div class="form-wrapper">
            <form  action="/createproject" class="form-horizontal" role="form" method="post">

                <div class="form-group">
                    <label for="inputSubject" class="col-sm-2 control-label">Subject:</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control input-sm" name="subject" id="inputSubject">
                    </div>
                </div>

                <div class="form-group">
                    <label for="inputSubject" class="col-sm-2 control-label">Name:</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control input-sm" name="name" id="inputName">
                    </div>
                </div>

                <div class="form-group">
                    <label for="inputSubject" class="col-sm-2 control-label">Submission Deadline:</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control input-sm" name="deadline" id="inputDeadline">
                    </div>
                </div>

                <div class="form-group">
                    <label for="inputSubject" class="col-sm-2 control-label">Description:</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control input-sm" name="description" id="inputDescription">
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-sm-offset-2">
                        <button type="submit" class="btn btn-primary btn-lg">Create</button>
                    </div>
                </div>


    </form>

</div>
<div id="chat-section">
    <%- partial('../chat/chat')%>
</div>

<%- partial('../partials/footer')%>